('original',array);\nconsole.log('splice',splice);\n// original [ 1, 5 ]\n// splice [ 2, 3, 4 ]\n\nconst array2 = [1,2,3,4,5];\nconst splice2 = array2.splice(1,3,'splice');\nconsole.log('original',array2);\nconsole.log('splice',splice2);\n// original [ 1, 'splice', 5 ]\n// splice [ 2, 3, 4 ]\n\n// [응용] 배열 요소를 합쳐보자\nconst array3 = [1,2,3,4,5];\nconst splice3 = array3.splice(1,3,...['splice1','splice2']);\nconsole.log('original',array3);\nconsole.log('splice',splice3);\n// original [ 1, 'splice1', 'splice2', 5 ]\n// splice [ 2, 3, 4 ]\n\n// [응용] 배열간 머지기능을 원하는 인덱스에 끼워줄수도 있다\nconst array4 = [1,2,3,4,5];\nconst splice4 = array4.splice(1,0,...['splice1','splice2']);\nconsole.log('original',array3);\nconsole.log('splice',splice3);\noriginal [ 1, 'splice1', 'splice2', 2, 3, 4, 5 ]\nsplice []"]],"language":[["JavaScript"]]},"created_time":1678103081836,"last_edited_time":1678151092534,"parent_id":"b8b11bd6-31fc-4536-9a0d-42ec9e86a91f","parent_table":"block","alive":true,"created_by_table":"notion_user","created_by_id":"7bab1fca-f898-4327-85ef-367dbf831f1d","last_edited_by_table":"notion_user","last_edited_by_id":"7bab1fca-f898-4327-85ef-367dbf831f1d","space_id":"45e5d7de-c052-4677-be01-f6ae18382d8a"}}},"space":{},"collection":{},"collection_view":{},"notion_user":{},"collection_query":{},"signed_urls":{},"preview_images":{"notion.so/image/pluralsight2.imgix.net%2Fpaths%2Fimages%2Fnodejs-45adbe594d.png":{"originalWidth":2000,"originalHeight":2000,"dataURIBase64":"data:image/webp;base64,UklGRpwAAABXRUJQVlA4WAoAAAAQAAAADwAADwAAQUxQSEwAAAARL0AkQJpjBkbNRkSQz4JVZNtpTug/BQFkgAWEDDTxb6bsIKL/E8Cfk7ro9ixPRdgIWDNEJ8HqkpaLzW8nE6OXQDQ2C6AbD8V5avkTVlA4ICoAAABwAQCdASoQABAABUB8JbACdAGgAADxZpEIP+VlhBN5DQ16Som0fK/wAAA="},"images.unsplash.com/photo-1488590528505-98d2b5aba04b":{"originalWidth":6000,"originalHeight":4000,"dataURIBase64":"data:image/webp;base64,UklGRjYAAABXRUJQVlA4ICoAAACwAQCdASoQAAsABUB8JQBOgCHIz1MgAP7pz65l3PBxS57UlWdKRM0JtgA="}}},"pageId":"b8b11bd6-31fc-4536-9a0d-42ec9e86a91f"}